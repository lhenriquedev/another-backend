    "@faker-js/faker": "^10.1.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/multipart": "^9.3.0",
    "@types/pg": "^8.15.5",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.8.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "fastify": "^5.6.0",
    "fastify-type-provider-zod": "^6.0.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "tsx": "^4.20.5",
