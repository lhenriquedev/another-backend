{
  "name": "another-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "node --env-file .env --experimental-strip-types --watch  src/server.ts",
    "start": "node --env-file=.env --experimental-strip-types src/server.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:prod": "node --experimental-strip-types src/database/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:seed": "node --experimental-strip-types src/database/seed.ts",
    "db:seed:local": "node --env-file .env --experimental-strip-types src/database/seed.ts",
    "db:setup": "npm run db:migrate:prod && npm run db:seed",
    "db:setup:local": "node --env-file .env --experimental-strip-types src/database/migrate.ts && node --env-file .env --experimental-strip-types src/database/seed.ts",
    "test": "vitest run"
  },
